{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "my_components/Results.jsx", "my_components/Card.jsx", "my_components/command.jsx", "my_components/GalleryDetails.jsx", "my_components/data.jsx", "my_components/AddFavoris.jsx", "my_components/searchCard.jsx", "my_components/signin-form.jsx", "my_components/signout-form.jsx", "my_components/signup-from.jsx", "my_components/skeletonCards.jsx", "my_components/datadiscover.jsx", "my_components/HeaderPage.jsx", "app/page.jsx"],
  "exclude": ["node_modules"]
}
